{
  "name": "tir-browser-auth-cdk",
  "version": "1.0.0",
  "description": "CDK infrastructure for TIR Browser auth service",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "deploy": "cdk deploy --require-approval never",
