    "diff": "cdk diff",
    "synth": "cdk synth"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.206.0",
    "constructs": "^10.4.2"
  },
  "devDependencies": {
    "@types/node": "^22.16.4",
    "aws-cdk": "^2.204.0",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "aws",
    "cdk",
    "tir-browser",
    "auth"
